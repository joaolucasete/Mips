{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "dashmips",
      "request": "launch",
      "name": "dashmips (Run Current File)",
      "program": "${file}",
      "registerFormat": "hex",
      "dashmipsCommand": "python -m dashmips debug",
      "dashmipsArgs": [
        "-l",
        "-i",
        "localhost",
        "-p",
        "8070"
      ]
    },
    {
      "type": "dashmips",
      "request": "attach",
      "name": "dashmips (Attach)",
      "registerFormat": "hex",
      "port": 8070,
      "host": "localhost"
    }
  ]
}